{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "strict": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "types": ["node", "obsidian"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}


